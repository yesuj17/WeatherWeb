<div id="PMSmodal" role="dialog" class="modal fade">
    <div style="width:1200px; background-color: white;" class="modal-dialog">
        <div style="width:100%; height:100%" class="modal-contents">
            <div style="height: 5%; background-color: #ffffff; color:#FFF" class="modal-header">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-11">
                            <h3 style="text-align:left; color:black" class="modal-title">PMS</h3>
                        </div>
                      <div class="col-sm-1">
                        <button class="close" data-dismiss="modal" type="button" style="color:red;" >
                          x
                        </button>
                      </div>
                    </div>
                </div>
            </div>
            <div ng-app="appPm" ng-controller="controllerpmgrid as DataTable" class="modal-body">
                <div style="" class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12"></div>
                        <div class="col-lg-1"></div>
                        <div id="header" style="display: table-row; height: 10%; width:100%;"></div>
                        <div style="margin: 0px; height: 100%">
						                <% include ./pms_head.ejs %>
                        </div>
                        <div id="content" style="display: table-row; height: 85%; width:100%;"></div>
                        <div id="left_content" style="height: 100px; width:10%; float:left; border:2px #f99 solid">
						                <% include ./pms_side.ejs %>
                        </div>
                        <div id="right_content" style="height: 100%; width:90%; float:right;border:2px #f99 solid">
						                <% include ./BodyFrame/body_index.ejs %>
                        </div>
                        <div id="footer" style="display: table-row; height: 5%; width:100%; border:2px #f99 solid">
                            <h6>CopyRight</h6>
                        </div>
                    </div>
                </div>
            </div>
            <script src="/pms/pmsController.js"></script>
        </div>
    </div>
</div>