            <div style="width: 100%;height: 100%;" class="container">
              <div id="userManagementModal" role="dialog" class="modal fade">
                <div class="modal-dialog">
                  <div style="width: 900px; height: auto; margin-left:-150px" class="modal-content"> 
                    <div class="modal-header">
                      <button type="button" data-dismiss="modal" class="close">X</button>
                      <h2 class="modal-title">User Management</h2>
                    </div>
                    <div class="modal-body">
                      <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="#List" ng-click="onClickTab('List')">List</a></li>
                        <li><a data-toggle="tab" ng href="#BarCode" ng-click="onClickTab('BarCode')">BarCode</a></li>
                      </ul>
                      <div class="tab-content">
                        <div id="List" class="tab-pane fade in active">
                          <nav role="query" class="navbar navbar-default query">
                            <div class="container-fluid">
                              <div class="collapse navbar-collapse">
                                <div class="navbar-form navbar-left">
                                  <select id="userFilter" name="size" class="form-control">
                                    <option value="UserName">Name</option>
                                    <option value="UserEmail">E-mail</option>
                                    <option value="UserPhone">Phone</option>
                                  </select>
                                  <input type="text" ng-model="searchText" class="form-control">
                                  <button ng-click="onSearchFilterUser()" class="btn btn-default">Search</button>
                                </div>
                                <div class="navbar-form navbar-right">
                                  <button id="userAddButton" ng-click="onOpenRegister()" class="btn btn-default">User Add</button><a id="userCancelButton" ng-click="onCloseRegister()" style="display:none;" class="btn btn-default">Cancel</a>
                                </div>
                              </div>
                            </div>
                          </nav>
                          <div style="display: none; width:auto;" class="container adduser">
                            <form name="registerForm" ng-submit="onRegisterFormSubmit(registerForm)">
                              <div class="form-group">
                                <input id="userRegisterButton" type="submit" name="submit_reg" value="Register" class="btn btn-default pull-right"><a id="userEditButton" ng-click="onEditRegister()" style="display:none;" class="btn btn-default pull-right">Edit</a><br><span class="req">* </span>User Name:
                                <input id="inputname" type="text" name="username" ng-model="userInfo.name" ng-change="onValidation('name')" placeholder="name" style="outline: none;" maxlength="18" required class="form-control">
                                <div ng-show="registerForm.username.$error.required" role="alert" class="col-xs-12 col-md-12 alert alert-warning">Please Enter User Name.</div>
                                <div ng-show="registerForm.username.$error.namevalid" role="alert" class="col-xs-12 col-md-12 alert alert-warning">This is not an User Name format.</div>
                                <div ng-show="registerForm.username.$error.duplicate" role="alert" class="col-xs-12 col-md-12 alert alert-warning">User Name already exists.</div>
                              </div>
                              <div class="form-group"><span class="req">* </span> Email Address:
                                <input type="text" name="useremail" ng-model="userInfo.email" ng-change="onValidation('email')" placeholder="email" style="outline: none;" maxlength="38" required class="form-control">
                                <div ng-show="registerForm.useremail.$error.required" role="alert" class="col-xs-12 col-md-12 alert alert-warning">Please Enter User E-mail Address.</div>
                                <div ng-show="registerForm.useremail.$error.emailvalid" role="alert" class="col-xs-12 col-md-12 alert alert-warning">This is not an E-mail Address format.</div>
                              </div>
                              <div class="form-group"><span class="req">* </span> Phone Number:
                                <input type="text" name="userphone" ng-model="userInfo.phone" ng-change="onValidation('phone')" placeholder="phone" style="outline: none;" required class="form-control">
                                <div ng-show="registerForm.userphone.$error.required" role="alert" class="col-xs-12 col-md-12 alert alert-warning">Please Enter User Phone Number.</div>
                                <div ng-show="registerForm.userphone.$error.phonevalid" role="alert" class="col-xs-12 col-md-12 alert alert-warning">This is not an Phone Number format. Please Enter without '-'</div>
                              </div>
                            </form>
                          </div>
                          <div class="table-responsive col-md-13"></div>
                          <table style="table-layout:fixed;" class="table table-striped table-hover text-left">
                            <thead></thead>
                            <tr>
                              <th class="text-left col-md-1">No</th>
                              <th class="text-left col-md-2">Name</th>
                              <th class="text-left col-md-4">E-mail</th>
                              <th class="text-left col-md-2">Phone</th>
                              <th class="text-left col-md-1">Edit</th>
                              <th class="text-left col-md-2">Delete</th>
                            </tr>
                            <tbody></tbody>
                            <tr ng-repeat="user in users">
                              <td class="col-md-1">
                                <p>{{user.UserIndex + 1}}</p>
                              </td>
                              <td class="col-md-2">
                                <p>{{user.UserName}}</p>
                              </td>
                              <td class="col-md-4">
                                <p>{{user.UserEmail}}</p>
                              </td>
                              <td class="col-md-2">
                                <p>{{user.UserPhone}}</p>
                              </td>
                              <td class="col-md-1">
                                <p><a href="#" ng-click="onEditUser($index)">
                                    <button type="button" class="btn btn-primary btn-xs">edit</button></a></p>
                              </td>
                              <td class="col-md-2">
                                <div id="{{myCarousel + user.UserName}}" data-ride="carousel" data-interval="false" class="carousel slide">
                                  <div class="carousel-inner">
                                    <div class="item active"><a href="#{{myCarousel + user.UserName}}" data-slide="next">
                                        <button type="button" class="btn btn-danger btn-xs">delete</button></a></div>
                                    <div class="item">
                                      <button type="button" ng-click="onDeleteUser($index)" class="btn btn-primary btn-xs">Yes</button><a href="#{{myCarousel + user.UserName}}" data-slide="prev" class="btn btn-success btn-xs">No</a>
                                    </div>
                                  </div>
                                </div>
                              </td>
                            </tr>
                          </table>
                        </div>
                        <div id="BarCode" class="tab-pane fade in">
                          <div class="table-responsive col-md-13"></div>
                          <table class="table table-striped table-hover">
                            <thead></thead>
                            <tr align="center">
                              <th>No</th>
                              <th>Name</th>
                              <th>Barcode</th>
                            </tr>
                            <tbody></tbody>
                            <tr ng-repeat="user in usersBarCode()">
                              <td>
                                <p>{{user.UserIndex + 1}}</p>
                              </td>
                              <td>
                                <p>{{user.UserName}}</p>
                              </td>
                              <td><img id="{{'myBarcode' + user.UserName}}"></td>
                            </tr>
                          </table>
                        </div>
                      </div>
                      <div style="margin-top: 10px;" class="form-inline text-center">
                        <button ng-click="onUserMore()" class="btn btn-default">More</button>
                      </div>
                    </div>
                    <div class="modal-footer">
                      <button type="button" data-dismiss="modal" class="btn btn-default">Close</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>